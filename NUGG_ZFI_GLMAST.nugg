  <method CLSNAME="ZCL_FI_GLMAST_API" CMPNAME="GET_SAKNR" VERSION="1" LANGU="E" DESCRIPT="Get GL Account" EXPOSURE="1" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
