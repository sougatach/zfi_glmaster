                  THEN me-&gt;check_auth_glmast( )
                  ELSE THROW zcx_fi_general(
                              textid  = zcx_fi_general=&gt;wrong_mode_fsv
                              gv_mode = me-&gt;get_mode( )
                                           )
                       ).

    me-&gt;check_data( ).

    r_result = me-&gt;_save( ).

  ENDMETHOD.</source>
