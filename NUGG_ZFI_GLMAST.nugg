  <method CLSNAME="ZCL_FI_GLMAST_API" CMPNAME="DEQUEUE" VERSION="1" LANGU="E" DESCRIPT="UnLock GL Account" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
