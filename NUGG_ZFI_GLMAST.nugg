*----------------------------------------------------------------------

    DATA(ls_acct_name) = VALUE glaccount_name(
                            keyy = VALUE #( BASE CORRESPONDING #( me-&gt;get_coa_refacct( ) )
                                             spras = sy-langu )
                                             ).

    CALL FUNCTION &apos;GL_ACCT_MASTER_GET_NAME&apos;
