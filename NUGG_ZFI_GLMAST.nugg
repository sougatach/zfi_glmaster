   <parameter CLSNAME="ZCL_IM_FI_QISR1_GL_MAINT" CMPNAME="GET_VIEW" SCONAME="R_RESULT" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="QISRDFORMVIEW"/>
