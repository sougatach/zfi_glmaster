   <source>METHOD get_domain_values.

    cl_reca_ddic_doma=&gt;get_values(
      EXPORTING
        id_name   =  iv_name
      IMPORTING
        et_values = DATA(lt_values)
      EXCEPTIONS
        not_found = 1
