                                             ELSE VALUE #( )
                                           )
                          )
                                                      ).

    CALL FUNCTION &apos;BAPI_COSTELEM_CHANGEMULTIPLE&apos;
      EXPORTING
        coarea          = me-&gt;get_coarea( )
        testrun         = me-&gt;get_simulate( )
      TABLES
        costelementlist = lt_costelem
        return          = lt_return.

    _mac_raise_multi lt_return.

    r_result = abap_true.

    me-&gt;_commit( ).

  ENDMETHOD.</source>
  </method>
