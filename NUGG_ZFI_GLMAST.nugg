   <parameter CLSNAME="ZCL_FI_FSV_API" CMPNAME="GET_VERSN" SCONAME="R_RESULT" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="VERSN_011"/>
